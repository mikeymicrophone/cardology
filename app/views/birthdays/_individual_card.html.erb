<%= div_for birthday, structural_role do %>
  <%= content_tag :header, @header_text %>
  <%= content_tag :header, @header_subtitle, :class => 'subtitle' %>
  <%= div_for card, :identification_of do %>
  <%= div_for card, :name_of do %>
    <%= card.name %>
  <% end %>
    <%= image_tag card.image, :class => 'card_face_image' %>
  <% end %>
  <%= div_for card, :explication_of do %>
    <%= mark_up card_explanation %>
  <% end %>
  <%= link_to @link_text, @alternative_card_location, :remote => true, :class => 'trailing call_to_action lunar_navigation' %>
<% end %>
