<p id="notice"><%= notice %></p>

<h1>Celestials</h1>

<table>
  <thead>
    <tr>
      <th>Birthday</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @celestials.each do |celestial| %>
      <tr>
        <td><%= celestial.birthday %></td>
        <td><%= celestial.name %></td>
        <td><%= link_to 'Show', celestial %></td>
        <td><%= link_to 'Edit', edit_celestial_path(celestial) %></td>
        <td><%= link_to 'Destroy', celestial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Celestial', new_celestial_path %>
